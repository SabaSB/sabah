name = "janis"
input_name = input("What is your name? ")
print(f"Hello {name}")

#output = f"what is your name? janis"

print("Please choose your action:")
actions = {1: "I'll read a book", 2: "I'llpractice in python", 3: "I'll take a rest"}

choice = int(input("Your Choice"))
if choice == 1:
    print(actions[1])
elif choice == 2:
    print(actions[2])
elif choice == 3:
    print(actions[3])
else:
    print("Invalid")
